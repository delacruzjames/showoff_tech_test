<h1>Fullname :<%= current_user.user['name']%></h1>
<h3>Email : <%= current_user.user['email']%></h3>
<h3>Date Of Birth : <%= current_user.user['date_of_birth']%></h3>
<h5>TOKEN : <span id="session_token" style="color: #df3626"><%= session[:token] %></span></h5>
<h3>Photo</h3>
<div class="img-avatar">
  <%= image_tag current_user.user["images"]["small_url"] %>
</div>
<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#updateMe">
  Update Details
</button>
<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#changePassword">
  Change Password
</button>
<%= link_to "Refresh Token", refresh_token_users_path, remote: true, class: "refresh btn btn-primary", method: :post %>

<%= render "change_password" %>
<%= render "update_me" %>
